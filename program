using System;

namespace s1
{
    class Program
    {
        static void Main(string[] args)
        { 
          int a,b,i,p;
          p = 1;
          Console.Write("input base : ");
          a = int.Parse(Console.ReadLine());
          Console.Write("input exponent : ");
          b = int.Parse(Console.ReadLine());
          for (i=1;i<=b;i++)
          {
              p = p*a;
          }
          Console.WriteLine("result is {0}",p);
          Console.WriteLine(" ");
        }
    }
}
